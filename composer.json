{
  "name": "kuriousagency/email-editor",
  "description": "Edit emails",
  "version": "2.0.1",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "emaileditor"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kurious Agency",
      "homepage": "https://kurious.agency"
    }
  ],
  "require": {
    "craftcms/cms": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "kuriousagency\\emaileditor\\": "src/"
    }
  },
  "extra": {
    "name": "Email Editor",
    "handle": "email-editor",
    "hasCpSettings": true,
    "hasCpSection": false,
    "components": {
      "emails": "kuriousagency\\emaileditor\\services\\Emails"
    },
    "class": "kuriousagency\\emaileditor\\EmailEditor"
  }
}
